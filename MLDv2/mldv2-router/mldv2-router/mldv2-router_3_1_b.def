#include "MLDv2R.def"

//-----------------------------------------------------------------------------
// TR1 -> All Nodes Address, MLDv2 Query (G) MRC 0 QRV 2 QQIC 125
//-----------------------------------------------------------------------------
FEM_icmp6_mldv2query_exth (
	MLDv2Q_tr1l_allnodes_m0_0_2_125,
	_HETHER_tr1_to_allnodes,
	{
		HopLimit        = MLDQ_HOP;
		_SRC(TR1_LLOCAL_UCAST);
		_DST(ALL_NODES_ADDR);
	},
	{
		header  = _HDR_IPV6_NAME(MLDv2Q_tr1l_allnodes_m0_0_2_125);
		exthdr  = ext_hbh_rtalert_mld;
		upper   = _ICMPV6_NAME(MLDv2Q_tr1l_allnodes_m0_0_2_125);
	},
	{
		Type                = MLDQ_TYPE;
		Code                = MLDQ_CODE;
		Checksum            = MLDQ_CHECKSUM;
		MaxResponseDelay    = 0;
		Reserved            = MLDQ_RESERVED;
		MulticastAddress    = MLDQG_MCADDR;
		Resv                = MLDQ_RESV;
		SFlag               = MLDQ_S_FLAG;
		QRV                 = 2;
		QQIC                = 125;
		NumOfSources        = 0;
	}
)

