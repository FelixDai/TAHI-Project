# Set the test version used for the display.
BEGIN { $V6evalTool::TestVersion = '$Name:  $';}

require SIP;

#--------------------------------------------------------------#
# INITIALIZATION
#--------------------------------------------------------------#
SIPScenarioModel('Server','UA-UA');
SIPInitializeScenario('','SKIP');

#--------------------------------------------------------------#
# SCENARIO Definition
#--------------------------------------------------------------#

sub SEQ_UserAgentCallee {
    my($node,$func);
    
    $node=NDDefStart('UA12');

    return 'END';

    NDDefEnd($node);
    
}



# 
sub SEQ_UserAgentCaller {
    my($node,$func);
    
    # 
    $node=NDDefStart('UA11');

	SIP_SetUp_Reg_Info('UA11', '');

    # UA
    $func=sub {
	my($result,$frame)=@_;
	
	# INVITE 
	SD_Term_INVITE('ES.REQUEST.INVITE.NOEXIST-ENTITY.TM');
#	NDVarDump($node);

	if(NDCFG('use_authorization') ne 'yes') {SEQ_Start('UA12');}

	return 'NEXT';
    };
    NDReqAction($node,$func,"START");

    # 407 
    $func=sub {
	my($result,$frame)=@_;
	
	JudgeResult($result,"Receive 407","Can't receive 407");
	
	# 407 
	SIP_Judgment('SS.STATUS.INVITE-407.TM');
	
	# ACK 
	SD_Term_ACK_NO200('ES.REQUEST.ACK-NO2XX.NOEXIST-ENTITY.TM');
	
	# INVITE 
	SD_Term_INVITEwithAuth('ES.REQUEST.INVITE.NOEXIST-ENTITY.AUTH.TM');
	
	# 
	SEQ_Start('UA12');
	
	return 'NEXT';
    };
    # 
    if(NDCFG('use_authorization') eq 'yes') {
	NDStatusAction($node,$func,"407",['D.Proxy-Authenticate.NONCE','D.TO-TAG','D.Proxy-Authenticate.OPAQUE']);
    }

    $func=sub {
	my($result,$frame)=@_;

#	$result=RV_Term_STATUS('404','INVITE');
	JudgeResult($result,"Receive 404","Can't receive 404");
	
	# 404 
	SIP_Judgment('SS.STATUS.INVITE-4XX.TM');
	print("4XX ERROR!!!!!!!!!!!!!!!");
#	exit;
	# ACK 
	SD_Term_ACK_NO200();
	return 'END';
	
    };
    NDStatusAction($node,$func,"404");

    NDDefEnd($node);
}

SEQ_UserAgentCallee();
SEQ_UserAgentCaller();
SEQ_Start('UA11');

#--------------------------------------------------------------#
# SCENARIO Start
