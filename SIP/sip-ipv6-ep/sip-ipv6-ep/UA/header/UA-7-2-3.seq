#--------------------------------------------------------------#
# ENTRANCE
#--------------------------------------------------------------#

# Set the test version used for the display.
BEGIN { $V6evalTool::TestVersion = '$Name:  $';}

require SIP;

#--------------------------------------------------------------#
# INITIALIZATION
#--------------------------------------------------------------#
SIPScenarioModel('Caller','2-Proxy');
SIPInitializeScenario();
SIPInitialAuthUse();

#--------------------------------------------------------------#
# SCENARIO
#--------------------------------------------------------------#

# Echo from Proxy
if(!$SIPLOGO){
SQ_Proxy_Echo();
}

WaitUntilKeyPressAndTelephone();

# Receive INVITE
$result=RV_Proxy_INVITE();
JudgeResult("Receive INVITE","Can't receive INVITE");

# Check INVITE
SIP_Judgment('SSet.REQUEST.INVITE.PX');

if($CNT_CONF{'AUTH-SUPPORT'} eq 'T'){

# Send 407
LOG_OK("Send 407");
$result=SD_Proxy_STATUS('407','INVITE');

# Receive ACK to 407
$result=RV_Proxy_ACK();
JudgeResult("Receive ACK","Can't receive ACK");

# Check ACK
SIP_Judgment('SSet.REQUEST.407-ACK.PX');

# Receive INVITE(with Proxy-Authorization)
$result=RV_Proxy_INVITEwithAuthorization();
JudgeResult("Receive INVITE with Authorization","Can't receive INVITE with Authorization");

# Check INVITE
SIP_Judgment('SSet.REQUEST.INVwithP-AUTH.PX');
}

# Send 100
LOG_OK("Send 100");
$result=SD_Proxy_STATUS('100','INVITE');

# Send 180
LOG_OK("Send 180");
$result=SD_Proxy_STATUS('180','INVITE');

# Recv rtp start
RtpPortOpen();

# Send 200
LOG_OK("Send 200");
$result=SD_Proxy_STATUS('200','INVITE');
$CNT_TUA_CONTACT_URI=FVal('HD.Contact.val.contact.ad.ad.txt');

# rtp start
$pid_rtp = RtpStart(0);

# Receive ACK(with Proxy-Authorization)
$result=RV_Proxy_ACK();
JudgeResult("Receive ACK","Can't receive ACK");

# Check ACK
SIP_Judgment('SSet.REQUEST.200-ACK.PX');

### Send And Receive RTP
#SQ_Term_RTP(5);

# Wait and Print "Key press and Hang up"
WaitUntilKeyPressAndHangup();

# Receive BYE
$result=RV_Proxy_BYE();
JudgeResult("Receive BYE","Can't receive BYE");

# Check BYE
SIP_Judgment('SSet.BYE.PX');


# rtp end
RtpStop();
ProcessEnd($pid_rtp);

# Send STATUS 200
LOG_OK("Send illegal 200(Contact include).");
$result=SD_Proxy_MSG(<<"SIP_HED",'',['E.VIA-RETURN','EESet.BASIC-FTCC-RETURN-TOTAG','E.CONTACT_PUA','E.CONTENT-LENGTH_0']);
SIP/2.0 200 OK
SIP_HED

MsgDisplay("Wait, while confirming BYE MUST NOT retransmit after illegal 200.");

# Receive BYE
$result = SQ_Proxy_SIP('BYE',10,40);

SIP_Judgment('SSet.RETRANS-NOBYE-AFTER3.BYE');

#--------------------------------------------------------------#
# Finish
#--------------------------------------------------------------#

SIPFinishScenario();

