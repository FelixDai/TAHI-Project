FEM_icmp6_destination_unreachable_exth(
	ICMPErrorFromPROXY,
	EH_TNtoNUT,
	{
		SourceAddress		= SIP_SEND_PSRV_SRC_ADDR;
		DestinationAddress	= SIP_SEND_NUT_DST_ADDR;
	},
	{
		header = _HDR_IPV6_NAME(ICMPErrorFromPROXY);
		upper = _ICMPV6_NAME(ICMPErrorFromPROXY);
	},
	{
		Type = ICMP_ERROR_TYPE;
		Code = ICMP_ERROR_CODE;
		payload = Received_SIP_Packet;
	}
)

