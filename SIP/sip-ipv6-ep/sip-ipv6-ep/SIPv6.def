	},
	{
		header = _HDR_IPV6_NAME(ICMPErrorFromOTHER1);
		upper = _ICMPV6_NAME(ICMPErrorFromOTHER1);
	},
	{
		Type = ICMP_ERROR_TYPE;
		Code = ICMP_ERROR_CODE;
		payload = Received_SIP_Packet;
	}
)

