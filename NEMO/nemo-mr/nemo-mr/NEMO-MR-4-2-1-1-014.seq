#--------------------------------------------------------------#
# ENTRANCE
#--------------------------------------------------------------#
# Set the test version used for the display.
BEGIN { $V6evalTool::TestVersion = '$Name: NEMO_1_0_2 $'; }

# Import the package.
use V6evalTool;
use mip6_mn_config;
use mip6_mn_common;
use mip6_mn_get;
use mip6_mn_send;
use mip6_mn_recv;
use mip6_mn_msg;

#--------------------------------------------------------------#
# INITIALIZATION
#--------------------------------------------------------------#
# Do the test satisfied the condition.
my @options = (
	'TEST_STATE_ABNORMAL',            'YES',                0,
	'TEST_FUNC_MPD',                  'YES',                0,
	'FUNC_DETAIL_MPD_RETRANSMISSION', 'YES',                0,
	'FUNC_DETAIL_MPD_RETRANSMISSION', 'NO',                 0,
);

# Initialize the state of TARGET.
Init_MN(@options);

#--------------------------------------------------------------#
# PREPARATION
#--------------------------------------------------------------#
# Prepare other local variables.
my @value;
my $sec;

#--------------------------------------------------------------#
# TEST PROCEDURE
#--------------------------------------------------------------#
#--------------------------------------------------------------#
# NUT moves from Link0 to LinkX.
#--------------------------------------------------------------#
# Receive Binding Update. (NUTX -> HA0)
# Send Binding Acknowledgement. (HA0 -> NUTX)
%packet = vRecv_to_move_from_Link0_to_LinkX_w_all_SA($IF0);
if ($packet{status} != 0) {
	$initcount ++;
	goto TERM;
}

#--------------------------------------------------------------#
vLogHTML_Info("<B>HA0 sends unsolicited Mobile Prefix Advertisement.</B>");
#--------------------------------------------------------------#
# Send unsolicited Mobile Prefix Advertisement. (HA0 -> NUTX with Type2 Routing Header)
$end_time = set_link_lifetime('Link0', 60, 60);
%packet_mpa = vSend_mpa($IF0, 'mpa_ha0ga_nutxga_rh2_nuthga_pfx_ha0', 0);

# Receive Mobile Prefix Solicitation. (NUTX -> HA0 with Home Address Option)
# Send Mobile Prefix Advertisement. (HA0 -> NUTX with Type2 Routing Header)
$interval = $PREFIX_ADV_TIMEOUT;
$retry = 0;
$now_time = time;
while ($now_time < $end_time) {

	%packet = vRecv_at_LinkX($IF0, $PREFIX_ADV_TIMEOUT,
		'mps_nutxga_ha0ga_hoa_nuthga',
		'bu_nutxga_ha0ga_hoa_nuthga');
	$now_time = time;

	if ($packet{recvFrame} eq 'bu_nutxga_ha0ga_hoa_nuthga') {
		last;
	}
	elsif ($packet{recvFrame} eq 'mps_nutxga_ha0ga_hoa_nuthga') {
		#--------------------------------------------------------------#
		vLogHTML_Info("<B>HA0 sends solicited Mobile Prefix Advertisement(invalid Identifier).</B>");
		#--------------------------------------------------------------#
		%value_mps_0 = get_value_of_mps(0, \%packet);
		$identifier = ($value_mps_0{Identifier} + 1) % 65536;
		@value = ("MPA_Identifier", $identifier);
		%packet_mpa = vSend_mpa($IF0, 'mpa_ha0ga_nutxga_rh2_nuthga_pfx_ha0', \%packet, @value);
		%packet_mps_0 = %packet;
		last;
	}
	else {
		# the retransmission of MPA is to 3 times.
		$wk = $now_time - $packet_mpa{sentTime1};
		if ($wk >= $interval) {
			if ($retry >= $PREFIX_ADV_RETRIES) {
				last;
			}
			#--------------------------------------------------------------#
			vLogHTML_Warn("<B>HA0 sends unsolicited Mobile Prefix Advertisement again.</B>");
			#--------------------------------------------------------------#
			%packet_pre_mpa = %packet_mpa;
			%packet_mpa = vSend_mpa($IF0, 'mpa_ha0ga_nutxga_rh2_nuthga_pfx_ha0', 0);
			$interval = ($packet_mpa{sentTime1} - $packet_pre_mpa{sentTime1}) * 2;
			$retry ++;
			$warncount ++;
		}
	}
}
if (%packet_mps_0 == 0) {
	vLogHTML_Fail("INIT FAIL: HA0 does not receive Mobile Prefix Solicitation.");
	$initcount ++;	
	goto TERM;
}


# Receive Mobile Prefix Solicitation. (NUTX -> HA0 with Home Address Option)
$sec = $end_time - $now_time;
%packet_mps_1 = vRecv_at_LinkX($IF0, $sec,
	'mps_nutxga_ha0ga_hoa_nuthga',
	'icmp6_pp_nutxga_ha0ga_pay_any',
	'icmp6_pp_nuthga_ha0ga_pay_any_tnl_nutx_ha0',
	'icmp6_pp_nutxga_ha0ga_hoa_nuthga_pay_any');

#--------------------------------------------------------------#
# JUDGEMENT
#--------------------------------------------------------------#
if ($packet_mps_1{recvFrame} eq 'mps_nutxga_ha0ga_hoa_nuthga') {
	vSend_mpa($IF0, 'mpa_ha0ga_nutxga_rh2_nuthga_pfx_ha0', \%packet_mps_1);
	vLogHTML_Pass("(*1) PASS: HA0 receives the retransmitted Mobile Prefix Solicitation.");
}
elsif (($packet_mps_1{recvFrame} eq 'icmp6_pp_nutxga_ha0ga_pay_any') ||
       ($packet_mps_1{recvFrame} eq 'icmp6_pp_nuthga_ha0ga_pay_any_tnl_nutx_ha0') ||
       ($packet_mps_1{recvFrame} eq 'icmp6_pp_nutxga_ha0ga_hoa_nuthga_pay_any')) {
	vLogHTML_Fail("(*1) FAIL: HA0 receives ICMP Parameter Problem without being silently ignored.");
	$failcount ++;
}
else {
	vLogHTML_Fail("(*1) FAIL: HA0 does not receive the retransmitted Mobile Prefix Solicitation.");
	$failcount ++;
	goto TERM;
}

TERM:
#--------------------------------------------------------------#
# TERMINATION
#--------------------------------------------------------------#
# Terminate the state of TARGET.
Term_MN($IF0, 1);

################################################################
__END__


