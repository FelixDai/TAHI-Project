BEGIN {}
END   {}

use mipv6;

my $movement	= 'USE_LINK0X';



$pktdesc{'local_ra_ha0'}			=
	'    Send RA (H=1) w/ HAI (hapref=0xffff) w/ PI0 (R=1) w/ PI1 (R=1): '.
	'HA0 (link-local) -&gt; all-nodes multicast address';

$pktdesc{'local_haad_reply_ha0_ha1_rut'}	=
	'    Recv HAAD Reply (HA0(a), HA0(b), RUT): RUT (Link0) -&gt; MN0X';

$pktdesc{'local_haad_reply_ha1_ha0_rut'}	=
	'    Recv HAAD Reply (HA0(b), HA0(a), RUT): RUT (Link0) -&gt; MN0X';



unless(mipv6init($movement)) {
	exitInitFail();
}



vLogHTML('<FONT COLOR="#FF0000" SIZE="5"><U><B>TEST PROCEDURE'.
	'</B></U></FONT><BR>');

my @rtadv	= ('local_ra_ha0');

if($HAVE_HOME_LINK) {
	unless(rtadv_exchange(@rtadv)) {
		exitFail();
	}

	#------------------------------#
	# exitCommon() control         #
	#------------------------------#
	$have_ha0_multiple_entry = $true;
}

unless(dhaad_confirmation($movement, 'haad_request',
	'local_haad_reply_ha0_ha1_rut', 'local_haad_reply_ha1_ha0_rut')) {

	exitFail();
}



exitPass();



#
# perldoc
#
########################################################################
__END__

