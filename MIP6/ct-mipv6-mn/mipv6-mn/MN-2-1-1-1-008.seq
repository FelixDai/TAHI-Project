#--------------------------------------------------------------#
# ENTRANCE
#--------------------------------------------------------------#
# Set the test version used for the display.
BEGIN { $V6evalTool::TestVersion = '$Name: MIPv6_4_0_5 $'; }

# Import the package.
use V6evalTool;
use mip6_mn_config;
use mip6_mn_common;
use mip6_mn_get;
use mip6_mn_send;
use mip6_mn_recv;
use mip6_mn_msg;

#--------------------------------------------------------------#
# INITIALIZATION
#--------------------------------------------------------------#
# Do the test satisfied the condition.
my @options = (
	'TEST_STATE_ABNORMAL',      'YES',                0,
	'TEST_FUNC_BASIC',          'YES',                0,
);

# Initialize the state of TARGET.
Init_MN(@options);

#--------------------------------------------------------------#
# PREPARATION
#--------------------------------------------------------------#
# Prepare other variables.
my @value;
my $sec;

#--------------------------------------------------------------#
# TEST PROCEDURE
#--------------------------------------------------------------#
#--------------------------------------------------------------#
# NUT starts at Link0.
#--------------------------------------------------------------#
# Send Router Advertisement. (HA0 -> HA0_allnode_multi)
vRecv_at_Link0($IF0, $FIRST_RA_TIME);

#--------------------------------------------------------------#
# NUT moves from Link0 to LinkX.
#--------------------------------------------------------------#
# Receive Binding Update. (NUTX -> HA0)
%packet = vRecv_to_detect_movement_at_LinkX($IF0, $BU_TIME);
if ($packet{status} != 0) {
	$initcount ++;
	goto TERM;
}

#--------------------------------------------------------------#
vLogHTML_Info("<B>HA0 sends Binding Acknowledgement, Status 0.</B>");
#--------------------------------------------------------------#
# Send Binding Acknowledgement. (HA0 -> NUTX)
$HA_BA_LIFETIME = 15;
vSend_ba($IF0, 'ba_ha0ga_nutxga_rh2_nuthga', \%RECV_PACKET_BU);

# for IKEv1
if (vRecv_to_establish_all_SA_at_LinkX($IF0) != 0) {
	$initcount ++;
	goto TERM;
}
%packet_bu = %RECV_PACKET_BU;

#--------------------------------------------------------------#
vLogHTML_Info("<B>HA0 sends ICMP Echo Request with Type2 Routing Header.</B>");
#--------------------------------------------------------------#
# Send ICMP Echo Request. (HA0 -> NUTX with Type2 Routing Header)
$id_no = 100;
@value = ('ECHO_IDENTIFIER', $id_no);
vSend_echorequest($IF0, 'echorequest_ha0ga_nutxga_rh2_nuthga', 0, @value);

# Receive ICMP Echo Reply. (NUTX -> HA0 with Home Address Option)
$now_time = time;
$end_time = $now_time + $ECHO_TIME;
while ($now_time < $end_time) {

	$sec = $end_time - $now_time;
	%packet = vRecv_at_LinkX($IF0, $sec,
		'bu_nutxga_ha0ga_hoa_nuthga',
		'echoreply_nutxga_ha0ga_hoa_nuthga'
		);
	$now_time = time;

	if ($packet{recvFrame} eq 'bu_nutxga_ha0ga_hoa_nuthga') {
		vSend_ba($IF0, 'ba_ha0ga_nutxga_rh2_nuthga', \%packet);
		%packet_bu = %packet;

		#--------------------------------------------------------------#
		vLogHTML_Info("<B>HA0 sends ICMP Echo Request with Type2 Routing Header.</B>");
		#--------------------------------------------------------------#
		$id_no ++;
		@value = ('ECHO_IDENTIFIER', $id_no);
		vSend_echorequest($IF0, 'echorequest_ha0ga_nutxga_rh2_nuthga', 0, @value);
	}
	elsif ($packet{recvFrame} eq 'echoreply_nutxga_ha0ga_hoa_nuthga') {
		%value_echo = get_value_of_echoreply(0, \%packet);
		if ($value_echo{'Identifier'} == $id_no) {
			$echo_flag = 1;
			last;
		}
	}
}
if ($echo_flag != 1) {
	vLogHTML_Fail("INIT FAIL: HA0 does not receive ICMP Echo Reply with Home Address Option.");
	$initcount ++;
	goto TERM;
}

$lifetime = $HA_BA_LIFETIME * 4;
#--------------------------------------------------------------#
vLogHTML_Info("<B>NUT out of all link. $lifetime [sec]</B>");
#--------------------------------------------------------------#
# (wait)
#   home registration is expired
$now_time = time;
$end_time = $now_time + $lifetime + $BU_MARGIN_TIME;
while ($now_time < $end_time) {
	set_now_link("NONE");
	vRecv($IF0, 1, 0, 1);
	$now_time = time;
	print(".");
}
print("\n");

#--------------------------------------------------------------#
vLogHTML_Info("<B>NUT return to LinkX.</B>");
#--------------------------------------------------------------#
# Receive Binding Update. (NUTX -> HA0)
%packet_bu_0 = vRecv_at_LinkX($IF0, $BU_TIME, 'bu_nutxga_ha0ga_hoa_nuthga');
if ($packet_bu_0{status} != 0) {
	goto JUDGE_1;
}

# Receive Binding Update. (NUTX -> HA0)
%packet_bu_1 = vRecv_at_LinkX($IF0, $BU_TIME, 'bu_nutxga_ha0ga_hoa_nuthga');
if ($packet_bu_1{status} == 0) {
	vSend_ba($IF0, 'ba_ha0ga_nutxga_rh2_nuthga', \%packet_bu_1);
}

JUDGE_1:
#--------------------------------------------------------------#
# JUDGEMENT (*1)
#--------------------------------------------------------------#
# check bu
if ($packet_bu_0{status} == 0) {
	vLogHTML_Pass("(*1) PASS: HA0 receives Binding Update.");
}
else {
	vLogHTML_Fail("(*1) FAIL: HA0 does not receive Binding Update.");
	$failcount ++;
	goto TERM;
}

# check retrans-bu
if ($MN_CONF{FUNC_DETAIL_BU_TO_HA_RETRANSMISSION} eq 'YES') {
	if ($packet_bu_1{status} == 0) {
		vLogHTML_Pass("(*2) PASS: HA0 receives the retransmitted Binding Update.");
	}
	else {
		vLogHTML_Info("(*2) INFO: HA0 does not receive the retransmitted Binding Update.");
		goto TERM;
	}
}
else {
	if ($packet_bu_1{status} != 0) {
		vLogHTML_Pass("(*2) PASS: HA0 does not receive Binding Update.");
		goto TERM;
	}
	else {
		vLogHTML_Info("(*2) INFO: HA0 receives the retransmitted Binding Update.");
	}
}

# check initial retransmission timer
$interval = $packet_bu_1{recvTime1} - $packet_bu_0{recvTime1};
if (check_backoff($interval, ($InitialBindackTimeoutFirstReg / 2)) == 0) {
	vLogHTML_Pass("The initial retransmission timer is set to InitialBindackTimeoutFirstReg. $interval [sec]");
}
else {
	vLogHTML_Fail("FAIL: The initial retransmission timer is not set to InitialBindackTimeoutFirstReg. $interval [sec]");
	$failcount ++;
}

TERM:
#--------------------------------------------------------------#
# TERMINATION
#--------------------------------------------------------------#
# Terminate the state of TARGET.
Term_MN($IF0, 1);

################################################################
__END__


